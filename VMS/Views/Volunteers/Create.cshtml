@model VMS.Models.Volunteer

@{
    ViewData["Title"] = "Create";
}

<div class="card">
    <div class="card-header">
        <h4>Create Volunteer</h4>
    </div>
    <div class="card-body">
        <div class="container-fluid">
            <div class="row">
                <div class="">
                    <form asp-action="Create">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-row">
                            <div class="form-group col-md-3">
                                <label asp-for="firstName" class="control-label text-primary">First Name</label>
                                <input asp-for="firstName" class="form-control" />
                                <span asp-validation-for="firstName" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-3">
                                <label asp-for="lastName" class="control-label text-primary">Last Name</label>
                                <input asp-for="lastName" class="form-control" />
                                <span asp-validation-for="lastName" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-3">
                                <label asp-for="userName" class="control-label text-primary">Username</label>
                                <input asp-for="userName" class="form-control" />
                                <span asp-validation-for="userName" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-3">
                                <label asp-for="password" class="control-label text-primary">Password</label>
                                <input asp-for="password" class="form-control" />
                                <span asp-validation-for="password" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-4">
                                <label asp-for="preferences" class="control-label text-primary">Preferences</label>
                                <input asp-for="preferences" class="form-control" />
                                <span asp-validation-for="preferences" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label asp-for="skills" class="control-label text-primary">Skills</label>
                                <input asp-for="skills" class="form-control" />
                                <span asp-validation-for="skills" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label asp-for="availability" class="control-label text-primary">Availability</label>
                                <input asp-for="availability" class="form-control" />
                                <span asp-validation-for="availability" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-4">
                                <label asp-for="address" class="control-label text-primary">Address</label>
                                <input asp-for="address" class="form-control" />
                                <span asp-validation-for="address" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label asp-for="phoneNumber" class="control-label text-primary">Phone#</label>
                                <input asp-for="phoneNumber" class="form-control" />
                                <span asp-validation-for="phoneNumber" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-4">
                                <label asp-for="email" class="control-label text-primary">Email</label>
                                <input asp-for="email" class="form-control" />
                                <span asp-validation-for="email" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label asp-for="education" class="control-label text-primary">Education</label>
                                <input asp-for="education" class="form-control" />
                                <span asp-validation-for="education" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-6">
                                <label asp-for="licenses" class="control-label text-primary">Licenses</label>
                                <input asp-for="licenses" class="form-control" />
                                <span asp-validation-for="licenses" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-2">
                                <label asp-for="approvalStatus" class="control-label text-primary">Approved?</label>
                                <input asp-for="approvalStatus" class="form-control" />
                                <span asp-validation-for="approvalStatus" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-2">
                                <label asp-for="activeStatus" class="control-label text-primary">Active Status</label>
                                <input asp-for="activeStatus" class="form-control" />
                                <span asp-validation-for="approvalStatus" class="text-danger"></span>
                            </div>
                        </div>
                        <p class="text-danger">Emergency Information</p>
                        <hr />
                        <div class="form-row">
                            <div class="form-group col-md-3">
                                <label asp-for="emergName" class="control-label text-danger">Emergency Contact</label>
                                <input asp-for="emergName" class="form-control" />
                                <span asp-validation-for="emergName" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-3">
                                <label asp-for="emergPhone" class="control-label text-danger">Phone</label>
                                <input asp-for="emergPhone" class="form-control" />
                                <span asp-validation-for="emergPhone" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-3">
                                <label asp-for="emergEmail" class="control-label text-danger">Email</label>
                                <input asp-for="emergEmail" class="form-control" />
                                <span asp-validation-for="emergEmail" class="text-danger"></span>
                            </div>
                            <div class="form-group col-md-3">
                                <label asp-for="emergAdd" class="control-label text-danger">Address</label>
                                <input asp-for="emergAdd" class="form-control" />
                                <span asp-validation-for="emergAdd" class="text-danger"></span>
                            </div>
                        </div>
                        <hr />

                        <div class="form-group">
                            <input type="submit" value="Create" class="btn btn-outline-success mr-3" />
                            <a class="btn btn-outline-primary" asp-action="Index">Back to List</a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>




